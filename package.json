{
  "name": "game-cncremastered",
  "version": "0.0.3",
  "description": "Adds support for C&C Remastered Collection mods in Vortex",
  "main": "./dist/index.js",
  "scripts": {
    "webpack": "node ./node_modules/webpack/bin/webpack --config webpack.config.js --display-error-details --progress --profile --color",
    "bundle7z": "7z a -t7z game-cncremastered.7z .\\dist\\index.js .\\dist\\info.json .\\gameart.png",
    "build": "npm run webpack && extractInfo && copyfiles -f ./gameart.png ./src/*.scss ./dist/"
  },
  "config": {
    "game": "Command & Conquer Remastered Collection"
  },
  "author": "agc93",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agc93/vortex-cncremastered.git"
  },
  "devDependencies": {
    "7z-bin": "Nexus-Mods/7z-bin",
    "@types/i18next": "^12.1.0",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-bootstrap": "^1.0.0",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "axios": "^0.19.2",
    "bluebird": "^3.5.5",
    "copyfiles": "^2.3.0",
    "electron": "4.2.4",
    "i18next": "^17.0.4",
    "react": "^16.8.6",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.13.1",
    "ts-loader": "^9.4.2",
    "typedoc": "^0.17.4",
    "typescript": "^3.5.2",
    "vortex-api": "github:Nexus-Mods/vortex-api",
    "vortex-ext-common": "^0.0.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {}
}
